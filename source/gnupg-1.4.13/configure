          esac
          ;;
      esac
      if test -z "$gt_cv_int_divbyzero_sigfpe"; then
        if test "$cross_compiling" = yes; then :

            # Guess based on the CPU.
            case "$host_cpu" in
              alpha* | i[34567]86 | x86_64 | m68k | s390*)
                gt_cv_int_divbyzero_sigfpe="guessing yes";;
